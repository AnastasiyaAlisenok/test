{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": ["airbnb-base", "prettier"],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "plugins": ["import", "prettier"],
    "rules": {
        "prettier/prettier": "error",
        "import/no-unresolved": [0],
        "import/export": 2,
        "import/extensions": 0
    },
    "ignorePatterns": ["webpack.config.js", "webpack.dev.config.js", "webpack.prod.config.js"]
}
